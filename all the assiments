function cToF(celsius) {
  // this is celsius to Fahrenheit
  var fahr = 0;
  fahr = (celsius* 1.8) + 32 
  console.log(celsius+"\xB0C is " + fahr + '\xB0F' );
 
}

function fToC(fahrenheit) {
var c = 6
fToC= ((5/9) * (fahrenheit - 32));

console.log(fahrenheit+"\xB0F is " + fToC + '\xB0c' );

  // this is Fahrenheit to Celsius
}

cToF(60);
fToC(45);


function computeAverageLengthOfWords(word1, word2) {
  // your code here
var sum1= word1.length;
var sum2= word2.length;
var output= (sum1+sum2)/2;
return output


} 
var output = computeAverageLengthOfWords('code', 'programs');
console.log(output); // --> 6


function checkAge(name, age) {
  if (age < 21)
    return("Go home, " + name + "!");


  if (age >= 21)
    return("Welcome, " + name + "!");
}


function isEvenAndGreaterThanTen(num) {
  // your code here
  if(num>10 && num%2 === 0)
  {
    return true;
  }
  else{
    return false
  }
}
var output = isEvenAndGreaterThanTen(13);
console.log(output); // --> false



function findMinLengthOfThreeWords(word1, word2, word3) {

  // your code here
  var sum1= word1.length;
  var sum2= word2.length;
  var sum3= word3.length;
  var george = 0
  if (sum1<sum2 && sum1<sum3) {
    return sum1;
   }
  if (sum2<sum1 && sum2<sum3) {
    return sum2;
   }
  if (sum3<sum1 && sum3<sum2) {
    return sum2;
   }

}
var output = findMinLengthOfThreeWords('a', 'be','see');
console.log(output); // --> 1

