var flips = prompt("Please enter a number");
var heads=0;
var tails=0;
if (flips != null) {
    console.log("Total coin Flips:" + flips);
    console.log('================');
}
for (i = 0; i < flips; i++) {

   var num = Math.floor(Math.random() * 100);
   if(num>50){
     heads+=1;}
    else{
      tails+=1;
    }
}
console.log('Heads'+ heads);
console.log('Tails'+ tails);
