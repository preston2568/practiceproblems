function acronymMaker(str){
  var word ="";
  word+=str[0];
  for(i=0;i<str.length;i++){
    if(str[i] == ' '){
      word+=(str[i+1]);
    }
 
  }
 
return word;
}

var output = acronymMaker("portable network graphics");
console.log(output);

17
function select(arr, obj) {
  // your code here
  var newObj = {};
  for(i=0;i<arr.length;i++){
    for(var key in obj){
      if(key == arr[i]){
        newObj[key] = obj[key];
      }
    }
  }
  return newObj;
}
var arr = ['a', 'c', 'e'];
var obj = {
  a: 1,
  b: 2,
  c: 3,
  d: 4
};
var output = select(arr, obj);
console.log(output); // --> { a: 1, c: 3 }

18
function computeFactorialOfN(n) {
  // your code here
var total=1;
  
  for(i=1;i<=n;i++){
    total*=i;
   
  }
  return total;
}

var output = computeFactorialOfN(5);
console.log(output); // --> 6

19
function noDuplicates(phrase){
  var splited=phrase.split(' ').sort();
  for(i=0;i<splited.length;i++){
    if (splited[i] == splited[i+1]) {
        return('no');
    }
  }
  return('yes');
}
var output = noDuplicates("IN THE RAIN AND THE SNOW");
console.log(output);

function convertScoreToGradeWithPlusAndMinus(score) {
  if (score>100 )
    return 'INVALID SCORE';
  if (score>89){
    if (score<93)
      return 'A-'
        if (score>97)
         return 'A+'
          else
            return 'A'}
  if (score>79){
    if (score<83)
      return 'B-'
        if (score>87)
         return 'B+'
          else
            return 'B'}
  if (score>69){
    if (score<73)
      return 'C-'
        if (score>77)
         return 'C+'
          else
            return 'C'}
  if (score>59){
    if (score<63)
      return 'B-'
        if (score>67)
         return 'D+'
          else
            return 'D'}
  if (score<0)
    return 'INVALID SCORE';
  else
    return "F"
}
var output = convertScoreToGradeWithPlusAndMinus(91);
console.log(output); // --> 'A-'
