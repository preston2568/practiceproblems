counter=[];

function owassoRams(n){
  var counter = [];
  for(x=1;x<n+1;x++){
   if(x%3===0 && x%5===0){
      counter.push('OwassoRams');
     
    }
    else if (x%3===0){
    counter.push('Owasso');
    }
    else if(x%5===0){
      counter.push('Rams');
    }
   
    else{
    counter.push(x);
     
    }
   
  }
  return counter
}
var output = owassoRams(15);
console.log(output);




stars=[];
function printStars(n) {
  for(x=0;x<n+1;x++){
    stars.push('*');
   
    console.log(stars);
   
  }
}
var output=printStars(4);
console.log(output);
